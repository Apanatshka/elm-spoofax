module Def

imports

Lexical
Type
Expr
Pattern

context-free syntax

Def.AnnotatedDef = [[TypeDef]
[ValueDef]]

TypeDef.NameAnno = [[ID] : [Type]]
TypeDef.OpAnno   = [([BINOP]) : [Type]]

ValueDef.PrefixOpDef = [([BINOP]) [Pattern*] = [Expr]]
ValueDef.InfixOpDef  = [[Pattern] [BINOP] [Pattern*] = [Expr]]
ValueDef.NameDef = [[ID] [Pattern*] = [Expr]]
Def = ValueDef
